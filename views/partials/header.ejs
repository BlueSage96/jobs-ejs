<h1>The Jobs EJS Application</h1>
<% if (errors) {
    errors.forEach((err) => { %>
        <!-- %= is what's going to be shown on screen -->
        <div>Error: <%= err %></div>
   <% }) } %>
   <% if (info) {
      info.forEach((msg) => { %>
         <div>Info: <%= msg %></div>
     <% }) } %>
<hr />